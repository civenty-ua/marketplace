        <span
            class="tabs__link tabs__icon-link"
            data-tab="my"
        >
            <svg viewBox="0 0 24 24">
                <rect x="11" y="3" width="2" height="18"></rect>
                <rect x="3" y="13" width="2" height="18" transform="rotate(-90 3 13)"></rect>
