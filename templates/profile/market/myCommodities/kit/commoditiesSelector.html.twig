<div class="personal-area__proposal-block">
    <div class="proposal-content">
        {{ form_row(form.commodities) }}
        <div
            class="tab marginTop0 visible"
            data-tab="all"
        >
            {% include 'profile/market/myCommodities/kit/commodities/all.html.twig' with {
                'form'          : form,
                'commodities'   : commodities.all,
            } only %}
        </div>
        <div
            class="tab marginTop0"
            data-tab="users"
        >
            <div class="users-list">
                {% include 'profile/market/myCommodities/kit/commodities/users.html.twig' with {
                    'form'  : form,
                    'users' : commodities.sellers,
                } only %}
            </div>
            <div class="user-commodities-list">
                {% include 'profile/market/myCommodities/kit/commodities/userCommodities.html.twig' with {
                    'form' : form,
                } only %}
            </div>
        </div>
        <div
            class="tab marginTop0"
            data-tab="my"
        >
            {% include 'profile/market/myCommodities/kit/commodities/my.html.twig' with {
                'form'          : form,
                'commodities'   : commodities,
            } only %}
        </div>
    </div>
    {% include 'profile/market/myCommodities/kit/preview/minimized.html.twig' with {
        'form' : form,
    } only %}
</div>
